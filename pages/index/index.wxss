  left:-5rpx;
  top:-20rpx;
  width:50rpx;
  height:50rpx;
  border-radius: 50%;
  border:10rpx rgba(200,200,200,0.6) solid;
  box-shadow:0 0 20rpx rgba(200,200,200,0.6);
  animation:ripple 1.5s ease infinite;
}
