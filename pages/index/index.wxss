}
@keyframes ripple {
  0% {transform:scale(0); opacity:0;}
  40% {transform:scale(0); opacity:1;}
  90% {transform:scale(1.4); opacity:1;}
  100% {transform:scale(1.5); opacity:0;}
}
@keyframes rippleimg {
  0% {transform:scale(1.2);}
  50% {transform:scale(1);}
  100% {transform:scale(1);}
}

