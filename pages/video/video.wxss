video {position:fixed; z-index:1; width:750rpx; height:1333.33rpx; bottom:-500vh; left:0;}
video.hiding_false {bottom:-71rpx;}
video.landscape {position:fixed; z-index:1; width:750rpx; height:100vh; top:-500vh; bottom:auto; left:0;}
video.hiding_false.landscape {top:0; bottom:auto;}
video.iphoneX {position:fixed; z-index:1; width:750rpx; height:100vh; top:-500vh; bottom:auto; left:0;}
video.hiding_false.iphoneX {top:0; bottom:auto;}

.qew{
  width:602rpx;
  background:rgba(0,0,0,.5);
  border-radius: 20rpx;
  padding:0 30rpx 58rpx;
  color:white;
  position: absolute;
  z-index:20000;
  top:50%;
  left:50%;
  transform: translate(-50%, -50%);
  transition:all 0.5s;
}

.qew .option{
  width:521rpx;
  height:85rpx;
  font-size: 30rpx;
  margin:58rpx auto 0 auto;
  display: flex;
  border-radius: 20rpx;
  background:rgba(255,255,255,.5);

}
.qew .circle{
  width:23rpx;
  height:23rpx;
  border-radius: 12rpx;
  background: white;
  position: relative;
  top:30rpx;
  left:18rpx;
}
.qew .title{
  padding-left:20rpx;
  height:85rpx;
  line-height: 85rpx;
}

.qew .options {padding:3vw; background-color:#FFF; color:#333; margin:1vw 2.5vw; border-radius:1vw;}

.qew .duration {width:100%; height:10rpx; margin:58rpx auto 0 auto; background-color:#AAA; border-radius:5rpx; overflow:hidden;}
.qew .duration cover-view {width:100%; height:10rpx; background-color:#DDD; border-radius:5rpx;}
@keyframes goRight {
  0% {transform:translateX(-100%);}
  100% {transform:translateX(0);}
}

.qewland{
  height:602rpx;
  background:rgba(0,0,0,.5);
  border-radius: 20rpx;
  padding:40rpx 58rpx;
  color:white;
  position: absolute;
  z-index:20000;
  top:50%;
  left:50%;
  transform: translate(-50%, -50%);
  transition:all 0.5s;
  box-sizing:border-box;
}
.qewland cover-image{
  width:86rpx;
  height:522rpx;
  margin-left:58rpx;
  float: right;
  border-radius: 20rpx;
  background-color:rgba(255,255,255,.5);
  background-position:top;
  background-repeat:no-repeat;
  background-size:100% auto;
}
.qewland cover-image:last-child {
  margin-left:0;
}

#controls {position:absolute; z-index:10000; top:0; right:0; bottom:0; left:0; background: rgba(0,0,0,.1);}

