.contactbox{
  position: relative;
  width: 100vw;
  height: 100vh;
  overflow: hidden;
  background-image: url('http://video.cdn.edisonluorui.com/filmimgs/index_back.jpg');
  background-size: 100% 100%;
}
.title{
  width: 438rpx;
  height: 101rpx;
  margin: 0 auto;
  margin-top: 96rpx;
}
.title image{
  width: 100%;
  height: 100%;
}
.code{
  width: 319rpx;
  height: 319rpx;
  margin: 0 auto;
  margin-top: 60rpx;
}
.code image{
  width: 100%;
  height: 100%;
}
.detailbox{
  margin-top: 57rpx;
  text-align: center;
}
.detail{
  font-size: 26rpx;
  color: #fff;
  line-height: 43rpx;
}
.feedbox{
  width: 515rpx;
  height: 159rpx;
  margin: 0 auto;
  font-size: 0;
  margin-top: 90rpx;
  border-radius: 80rpx;
  background: #3D2052;
}
.introbox{
  float: left;
  margin-top: 49rpx;
  margin-left: 87rpx;
}
.intro{
  text-align: right;
  font-size:24rpx;
  color: #fff;
  line-height: 38rpx;
}
.feedbtn{
  float: left;
  width: 114rpx;
  height: 114rpx;
  margin-top: 23rpx;
  margin-left: 50rpx;
  line-height: 114rpx;
  text-align: center;
  border-radius: 50%;
  background: #ffd400;
  font-size: 26rpx;
  color: #925d00;
  font-weight: 700;
}